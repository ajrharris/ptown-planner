{
  "name": "ptown-planner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create ptown-planner-db",
    "db:migrate": "wrangler d1 execute ptown-planner-db --file=schema.sql",
    "db:migrate:local": "wrangler d1 execute ptown-planner-db --local --file=schema.sql"
  },
  "devDependencies": {
    "wrangler": "^4"
  }
}
